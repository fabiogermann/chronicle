# Test Server Credentials Template
# Copy this file to test-credentials.properties and fill in your values
# NEVER commit test-credentials.properties to version control

# ============================================================================
# Plex Demo Server
# ============================================================================
# Server URL (including port if non-standard)
plex.server.url=https://your-plex-server.example.com:32400

# Plex authentication token (from X-Plex-Token)
plex.server.token=YOUR_PLEX_TOKEN

# Library section ID containing audiobooks
plex.library.id=1

# ============================================================================
# Audiobookshelf Demo Server
# ============================================================================
# Server URL
abs.server.url=https://your-audiobookshelf-server.example.com

# Authentication (use either username/password OR api.key)
abs.username=test_user
abs.password=test_password

# Alternative: API key authentication
abs.api.key=YOUR_API_KEY

# Library ID for testing
abs.library.id=lib_default
